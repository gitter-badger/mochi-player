QTabWidget::tab-bar { alignment: center; }

/* Style the tab using the tab sub-control. Note that
    it reads QTabBar _not_ QTabWidget */
QTabBar::tab {
	border-style: none;
	padding-bottom: 5px;
	min-width: 100px;
}
QTabBar::tab:selected {
	border-bottom: 1px solid #008098;
	font-weight: bold;
}
QTabBar::tab:!selected {
	color: #B8B8B8;
	font-weight: normal;
	margin-bottom: 1px;
}</string>
